{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://ehrms.seq:5341"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "MessageBroker": {
    "Host": "amqp://ehrms-mq:5672",
    "Username": "guest",
    "Password": "guest"
  },
  "ConnectionStrings": {
    "ProjectManagementDb": "Server=ehrms.projectmanagement.api.db,1433;Initial Catalog=UsersDatabase;user id=sa;pwd=yourStrong(!)Password;Trusted_Connection=False;TrustServerCertificate=True"
  }
}