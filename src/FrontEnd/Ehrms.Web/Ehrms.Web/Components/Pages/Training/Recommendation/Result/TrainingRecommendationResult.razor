<MudTable Items="@RecommendationResult.Employees" Dense="false">
    <ToolBarContent>
        <div class="d-flex justify-content-end">
            <MudTooltip Text="Add Training">
                <MudIconButton Size="Size.Medium" Color="Color.Success" Icon="@Icons.Material.Filled.AddBox"/>
            </MudTooltip>
        </div>
    </ToolBarContent>
    <RowTemplate>
        <MudTd>
            <MudLink Href=@($"{EmployeeRouting.Details}/{@context.Id}")>
                @context.FullName
            </MudLink>
        </MudTd>
    </RowTemplate>
</MudTable>

@code {
    [Parameter] public ReadTrainingRecommendationResultModel RecommendationResult { get; set; }
}